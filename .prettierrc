{
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 120,
  "bracketSpacing": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "editor.formatOnSave": true,
  "prettier.semi": true,
  "prettier.eslintIntegration": true,
  "jsxBracketSameLine": false,
  "endOfLine": "auto",
  "tabWidth": 2,
  "useTabs": false,
  "parser": "typescript",
  "overrides": [
    {
      "files": "*.scss",
      "options": {
        "parser": "css"
      }
    },
    {
      "files": "*.less",
      "options": {
        "parser": "css"
      }
    }
  ]
}
