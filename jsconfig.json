{
  "compilerOptions": {
    "baseUrl": "./src",
    "jsx": "preserve",
    "paths": {
      "@/*": ["./src/*"],
      "@/components/*": ["./components/*"],
      "@/config/*": ["./config/*"],
      "@/container/*": ["./container/*"],
      "@/i18n/*": ["./i18n/*"],
      "@/layout/*": ["./layout/*"],
      "@/pages/*": ["./pages/*"],
      "@/redux/*": ["./redux/*"],
      "@/routes/*": ["./routes/*"],
      "@/static/*": ["./static/*"],
      "@/utils/*": ["./utils/*"],
      "@/mock/*": ["./mock/*"]
    }
  },
  "exclude": ["node_modules", "build"]
}
